{
  "name": "react-native-text-watcher",
  "displayName": "React Native Text Watcher",
  "description": "Detects and Warns you unwrapped text strings in React Native projects.",
  "version": "1.0.0",
  "author": {
    "name": "Sugam Subedi",
    "email": "sugamsubedi234@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/supSugam/react-native-text-watcher"
  },
  "capabilities": {
    "languages": [
      {
        "id": "typescriptreact",
        "extensions": [
          ".tsx"
        ]
      },
      {
        "id": "javascriptreact",
        "extensions": [
          ".jsx"
        ]
      }
    ]
  },
  "icon": "images/logo.png",
  "keywords": [
    "react-native",
    "text-watcher",
    "react-native-text-watcher",
    "react-native-text",
    "react-native-text-detection",
    "text-warning"
  ],
  "homepage": "https://www.github.com/supSugam/react-native-text-watcher",
  "publishConfig": {
    "access": "public"
  },
  "publisher": "supSugam",
  "readme": "README.md",
  "bugs": {
    "url": "https://www.github.com/supSugam/react-native-text-watcher/issues"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Linters",
    "Debuggers",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0"
  },
  "dependencies": {
    "typescript": "^5.4.5",
    "vscode-languageclient": "^9.0.1"
  }
}
